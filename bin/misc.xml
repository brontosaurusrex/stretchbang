<openbox_pipe_menu>
<item label="Blender">
    <action name="Execute">
        <execute>~/apps/blender/blender</execute>
    </action>
</item>
<item label="gimp">
    <action name="Execute">
        <execute>gimp</execute>
    </action>
</item>
<item label="inkscape">
    <action name="Execute">
        <execute>inkscape</execute>
    </action>
</item>
<item label="krita">
